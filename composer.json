{
    "name": "reshadman/laravel-committed",
    "description": "Adds **committed** event to Eloquent default event callbacks (created, creating, saved, saving...).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Reza Shadman",
            "email": "pcfeeler@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/support": "~5.5.0|~5.6.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Reshadman\\Committed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reshadman\\Committed\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reshadman\\Committed\\CommittedServiceProvider"
            ]
        }
    },
}
